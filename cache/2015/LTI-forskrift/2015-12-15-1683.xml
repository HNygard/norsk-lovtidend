        <tgroup cols="3" rowsep="0" colsep="0">
          <colspec colname="1" align="left" valign="top" colwidth="56*"/>
          <colspec colname="2" align="right" valign="top" colwidth="6*"/>
          <colspec colname="3" align="center" valign="top" colwidth="14*"/>
          <tbody>
            <row>
              <entry colname="1">Godkjenning av endring av dyreforsøk </entry>
              <entry colname="2">1 440 </entry>
