          <thgroup cols="3" rowsep="0" colsep="0">
            <colspec colname="1" align="left" valign="top" colwidth="35*"/>
            <colspec colname="2" align="left" valign="top" colwidth="20*"/>
            <colspec colname="3" align="left" valign="top" colwidth="20*"/>
            <tbody>
              <row>
                <entry colname="1">
                  <i>Kommune/
<br/> Del av kommunen</i>
                </entry>
                <entry colname="2">
                  <i>Elg:
<br/> Minsteareal i daa</i>
                </entry>
                <entry colname="3">
                  <i>Rådyr:
<br/> Minsteareal i daa</i>
                </entry>
              </row>
            </tbody>
          </thgroup>
          <tgroup cols="3" rowsep="0" colsep="0">
            <colspec colname="1" align="left" valign="top" colwidth="35*"/>
            <colspec colname="2" align="left" valign="top" colwidth="20*"/>
            <colspec colname="3" align="left" valign="top" colwidth="20*"/>
            <tbody>
              <row>
                <entry colname="1">1804V0086 - Kjerringøy elgvald </entry>
                <entry colname="2">3000 </entry>
                <entry colname="3"> </entry>
              </row>
              <row>
                <entry colname="1">1842V0001 - Vestersida elgvald
<br/> 1842J0002 - Breivik jaktfelt
<br/> 1804V0088 - Bodø sør elgvald </entry>
