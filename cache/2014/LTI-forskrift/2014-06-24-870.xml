        <thgroup cols="3" rowsep="0" colsep="0">
          <colspec colname="1" align="left" valign="top" colwidth="15*"/>
          <colspec colname="2" align="left" valign="top" colwidth="46*"/>
          <colspec colname="3" align="left" valign="top" colwidth="15*"/>
          <tbody>
            <row>
              <entry colname="1">
                <i>Takstnummer</i>
              </entry>
