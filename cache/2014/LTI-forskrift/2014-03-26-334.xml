          <tbody>
            <row>
              <entry colname="1"/>
              <entry colname="2">1. </entry>
              <entry colname="3"/>
              <entry colname="4">fornyelsesavgift for beskyttelse i inntil tre klasser </entry>
              <entry colname="5">kr </entry>
              <entry colname="6">2600, </entry>
              <entry colname="7"/>
              <entry colname="8">og </entry>
            </row>
            <row>
