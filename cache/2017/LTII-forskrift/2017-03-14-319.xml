        <thgroup cols="5" rowsep="0" colsep="0">
          <colspec colname="1" align="left" valign="top" colwidth="15*"/>
          <colspec colname="2" align="left" valign="top" colwidth="15*"/>
          <colspec colname="3" align="left" valign="top" colwidth="15*"/>
          <colspec colname="4" align="left" valign="top" colwidth="15*"/>
          <colspec colname="5" align="left" valign="top" colwidth="15*"/>
          <tbody>
            <row>
              <entry colname="1">
                <i>Viltart</i>
              </entry>
              <entry colname="2">
                <i>Elg</i>
              </entry>
              <entry colname="3">
                <i>Hjort</i>
              </entry>
              <entry colname="4">
                <i>Rådyr</i>
              </entry>
              <entry colname="5">
                <i>Bever</i>
              </entry>
            </row>
          </tbody>
        </thgroup>
        <tgroup cols="5" rowsep="0" colsep="0">
          <colspec colname="1" align="left" valign="top" colwidth="15*"/>
          <colspec colname="2" align="left" valign="top" colwidth="15*"/>
          <colspec colname="3" align="left" valign="top" colwidth="15*"/>
          <colspec colname="4" align="left" valign="top" colwidth="15*"/>
          <colspec colname="5" align="left" valign="top" colwidth="15*"/>
          <tbody>
            <row>
              <entry colname="1">Minsteareal i dekar </entry>
              <entry colname="2">4 500 </entry>
              <entry colname="3">10 000 </entry>
              <entry colname="4">1 100 </entry>
              <entry colname="5"> </entry>
            </row>
            <row>
              <entry colname="1">Fordelingsgrunnlag </entry>
              <entry colname="2"/>
              <entry colname="3"/>
              <entry colname="4"/>
              <entry colname="5">Areal </entry>
            </row>
          </tbody>
        </tgroup>
      </table>
    </paragraf>
    <paragraf id="/paragraf/3" url="https://lovdata.no/lovtidend/forskrift/2017-03-14-319/p3">
      <pverdi>§ 3.</pverdi>
      <ledd id="/paragraf/3/ledd/1">Denne forskrift trer i kraft straks. Samtidig oppheves forskrift 24. mars 2010 nr. 475 om adgang til jakt etter elg, hjort, rådyr og bever, Vennesla kommune, Vest-Agder.</ledd>
    </paragraf>
