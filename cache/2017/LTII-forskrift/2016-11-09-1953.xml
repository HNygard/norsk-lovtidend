      <paragraf id="/kapittel/2/paragraf/6" url="https://lovdata.no/lovtidend/forskrift/2016-11-09-1953/k2/p6">
        <pverdi>§ 6.</pverdi>
        <ptittel>Opplæringsløpet har følgende flytting av fag:</ptittel>
        <liste id="/kapittel/2/paragraf/6/ledd/1/liste/1">
          <lverdi>-</lverdi>
          <avsnitt id="/kapittel/2/paragraf/6/ledd/1/liste/1/avsnitt/1">Norskfaget fordeles jevnere over de tre årene, faget følger studieforberedende modell over tre år</avsnitt>
        </liste>
        <liste id="/kapittel/2/paragraf/6/ledd/1/liste/2">
          <lverdi>-</lverdi>
