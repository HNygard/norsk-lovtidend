          <colspec colname="2" align="center" valign="top" colwidth="25*"/>
          <tbody>
            <row>
              <entry colname="1">
                <i>Største lengde</i>
              </entry>
              <entry colname="2">
                <i>Herav garantert kvote</i>
              </entry>
            </row>
          </tbody>
        </thgroup>
        <tgroup cols="2" rowsep="0" colsep="0">
          <colspec colname="1" align="center" valign="top" colwidth="25*"/>
          <colspec colname="2" align="center" valign="top" colwidth="25*"/>
          <tbody>
            <row>
              <entry colname="1">Under 8 m </entry>
              <entry colname="2">14 </entry>
            </row>
            <row>
              <entry colname="1">8-9,9 m </entry>
              <entry colname="2">20 </entry>
            </row>
            <row>
              <entry colname="1">10 m og over </entry>
              <entry colname="2">22 </entry>
            </row>
