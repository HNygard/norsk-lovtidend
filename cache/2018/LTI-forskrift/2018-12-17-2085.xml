          <colspec colname="5" align="right" valign="top" colwidth="9*"/>
          <tbody>
            <row>
              <entry colname="1"/>
              <entry colname="2">a) </entry>
              <entry colname="3">Kjøretøy i gruppe L </entry>
              <entry colname="4">Kr </entry>
              <entry colname="5">610,- </entry>
            </row>
            <row>
              <entry colname="1"/>
              <entry colname="2">b) </entry>
              <entry colname="3">Kjøretøy i gruppe L, fornyet søknad med begrenset kontroll </entry>
              <entry colname="4">Kr </entry>
