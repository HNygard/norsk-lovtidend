          <tbody>
            <row>
              <entry colname="1"/>
              <entry colname="2">
                <i>Kroner per heltidsplass driftsutgifter, inklusive administrasjonspåslag</i>
              </entry>
            </row>
          </tbody>
        </thgroup>
        <tgroup cols="2" rowsep="0" colsep="0">
          <colspec colname="1" align="left" valign="top" colwidth="38*"/>
          <colspec colname="2" align="left" valign="top" colwidth="38*"/>
          <tbody>
            <row>
              <entry colname="1">Ordinære barnehager, barn fra null år til to år </entry>
