<liste id="/kapittel/i/ledd/4/liste/f"><lverdi>f)</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f/avsnitt/1">tvangsmulkt ilagt med hjemmel i:</avsnitt><liste id="/kapittel/i/ledd/4/liste/f-1"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-1/avsnitt/1">el-tilsynsloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-2"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-2/avsnitt/1">finanstilsynsloven § 10,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-3"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-3/avsnitt/1">produktkontrolloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-4"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-4/avsnitt/1">forurensningsloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-5"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-5/avsnitt/1">finansieringsvirksomhetsloven § 5-2,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-6"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-6/avsnitt/1">statistikkloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-7"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-7/avsnitt/1">pengespilloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-8"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-8/avsnitt/1">kringkastingsloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-9"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-9/avsnitt/1">legemiddelloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-10"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-10/avsnitt/1">luftfartsloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-11"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-11/avsnitt/1">lotteriloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-12"><lverdi>-</lverdi><avsnitt id="/kapittel/i/ledd/4/liste/f-12/avsnitt/1">medieeierskapsloven,</avsnitt></liste><liste id="/kapittel/i/ledd/4/liste/f-13"><lverdi>-</l