              <entry colname="4">
                <lenke url="/ltavd1/filer/grafikk/sf-20160210-0133-01-01.pdf">Vedlegg 1</lenke>
              </entry>
            </row>
          </tbody>
        </tgroup>
      </table>
    </kapittel>
    <kapittel type="kapittel" id="/kapittel/ii" url="https://lovdata.no/lovtidend/forskrift/2016-02-10-133/kii">
      <kverdi>II</kverdi>
      <ktittel/>
      <ledd id="/kapittel/ii/ledd/1">Endringen trer i kraft 10. februar 2016.</ledd>
