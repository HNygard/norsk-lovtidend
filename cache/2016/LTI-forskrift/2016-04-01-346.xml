          <colspec colname="2" align="center" valign="top" colwidth="38*"/>
          <tbody>
            <row>
              <entry colname="1">
                <i>Største lengde</i>
              </entry>
              <entry colname="2">
                <i>Garantert kvote</i>
              </entry>
            </row>
          </tbody>
        </thgroup>
        <tgroup cols="2" rowsep="0" colsep="0">
          <colspec colname="1" align="left" valign="top" colwidth="38*"/>
          <colspec colname="2" align="center" valign="top" colwidth="38*"/>
          <tbody>
            <row>
              <entry colname="1">Under 8 m </entry>
              <entry colname="2">15 </entry>
            </row>
            <row>
              <entry colname="1">8-9,99 m </entry>
              <entry colname="2">21 </entry>
            </row>
            <row>
              <entry colname="1">10 m og over </entry>
              <entry colname="2">24 </entry>
            </row>
