<liste id="/paragraf/2/ledd/1/liste/e"><lverdi>e)</lverdi><avsnitt id="/paragraf/2/ledd/1/liste/e/avsnitt/1">Iverryggen, avgrenset av rette linjer trukket mellom følgende posisjoner:</avsnitt><liste id="/paragraf/2/ledd/1/liste/e.1"><lverdi>1.</lverdi><avsnitt id="/paragraf/2/ledd/1/liste/e.1/avsnitt/1">N 64° 55,0′ E 009° 02,0′</avsnitt></liste><liste id="/paragraf/2/ledd/1/liste/e.2"><lverdi>2.</lverdi><avsnitt id="/paragraf/2/ledd/1/liste/e.2/avsnitt/1">N 64° 55,0′ E 009° 30,0′</avsnitt></liste><liste id="/paragraf/2/ledd/1/liste/e.3"><lverdi>3.</lverdi><avsnitt id="/paragraf/2/ledd/1/liste/e.3/avsnitt/1">N 65° 10,0′ E 009° 30,0′</avsnitt></liste><liste id="/paragraf/2/ledd/1/liste/e.4"><lverdi>4.</lverdi><avsnitt id="/paragraf/2/ledd/1/liste/e.4/avsnitt/1">N 65° 10,0′ E 009° 20,0′</avsnitt></liste><liste id="/paragraf/2/ledd/1/liste/e.5"><lverdi>5.</lverdi><avsnitt id="/paragraf/2/ledd/1/liste/e.5/avsnitt/1">N 65° 06,0′ E 009° 08,0′</avsnitt></liste></liste>
