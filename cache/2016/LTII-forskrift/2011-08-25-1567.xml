            <colspec colname="4" align="left" valign="top" colwidth="18*"/>
            <tbody>
              <row>
                <entry colname="1">
                  <i>Tankvolum inntil</i>
                </entry>
                <entry colname="2">
                  <i>Årsgebyr</i>
                </entry>
                <entry colname="3">
                  <i>Grunngebyr</i>
                </entry>
                <entry colname="4">
                  <i>Grunnlag</i>
                </entry>
              </row>
            </tbody>
          </thgroup>
          <tgroup cols="4" rowsep="0" colsep="0">
            <colspec colname="1" align="left" valign="top" colwidth="19*"/>
            <colspec colname="2" align="left" valign="top" colwidth="19*"/>
