                <entry colname="5">Y </entry>
              </row>
              <row>
                <entry colname="1">10 m<sup>3</sup> </entry>
                <entry colname="2">A = </entry>
                <entry colname="3">G </entry>
                <entry colname="4">5,0 </entry>
                <entry colname="5">Y </entry>
